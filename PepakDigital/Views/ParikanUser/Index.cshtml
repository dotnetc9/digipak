@model IEnumerable<PepakDigital.Parikan>

@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_LayoutwithSidebar.cshtml";
}

<br />
<div class="col-lg-15">
		  <div class="panel panel-primary">
			<div class="panel-heading"><center><h4>Parikan</h4></center></div>
			<div class="panel-body">
				<table class="table table-striped table-hover">
                <thead>
                        <tr>
                        <th>
                            @Html.DisplayNameFor(model => model.IsiParikan)
                        </th>
       
                        <th></th>
                    </tr>
                 </thead>
                 <tbody>
                @foreach (var item in Model) {
                    <tr>
                        <td>
                            @Html.DisplayFor(modelItem => item.IsiParikan)
                        </td>
        
                    </tr>
                }
                </tbody>
				</table>
		    </div><!--/panel-body-->
</div><!--/panel-->