@model PepakDigital.Paribasan

@{
    ViewBag.Title = "Edit";
    Layout = "~/Views/Shared/_LayoutUser.cshtml";
}

<h2>Edit</h2>

@using (Html.BeginForm())
{
    @Html.AntiForgeryToken()
    
    <div class="form-horizontal">
        <h4>Paribasan</h4>
        <hr />
        @Html.ValidationSummary(true)
        @Html.HiddenFor(model => model.ParibasanId)

        <div class="form-group">
            @Html.LabelFor(model => model.IsiParibasan, new { @class = "control-label col-md-2" })
            <div class="col-md-10">
                @Html.EditorFor(model => model.IsiParibasan)
                @Html.ValidationMessageFor(model => model.IsiParibasan)
            </div>
        </div>

        <div class="form-group">
            @Html.LabelFor(model => model.Artine, new { @class = "control-label col-md-2" })
            <div class="col-md-10">
                @Html.EditorFor(model => model.Artine)
                @Html.ValidationMessageFor(model => model.Artine)
            </div>
        </div>

        <div class="form-group">
            @Html.LabelFor(model => model.KategoriId, "KategoriId", new { @class = "control-label col-md-2" })
            <div class="col-md-10">
                @Html.DropDownList("KategoriId", String.Empty)
                @Html.ValidationMessageFor(model => model.KategoriId)
            </div>
        </div>

        <div class="form-group">
            <div class="col-md-offset-2 col-md-10">
                <input type="submit" value="Save" class="btn btn-default" />
            </div>
        </div>
    </div>
}

<div>
    @Html.ActionLink("Back to List", "Index")
</div>
