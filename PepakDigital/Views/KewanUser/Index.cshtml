@model IEnumerable<PepakDigital.Kewan>

@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_LayoutUser.cshtml";
}

<div class="col-md-6">
		  <div class="panel panel-default">
					  <div class="panel-heading"><h4>ARANE ANAK KEWAN</h4></div>
						<div class="panel-body">
							<table class="table table-striped">
							<thead>
                                <tr><th> @Html.DisplayNameFor(model => model.JenengKewan)</th>
                                    <th> @Html.DisplayNameFor(model => model.AnakeKewan)</th>
                                </tr>
                            </thead>
                            <tbody>
								@foreach (var item in Model) {
                                    <tr>
                                        <td>
                                            @Html.DisplayFor(modelItem => item.JenengKewan)
                                        </td>
                                        <td>
                                            @Html.DisplayFor(modelItem => item.AnakeKewan)
                                        </td>
                                    </tr>
                                }
							</tbody>
							</table>
						</div><!--/panel-body-->
           </div><!--/panel-->